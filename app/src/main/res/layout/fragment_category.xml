<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.tpicy.CategoryFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:id="@+id/pickle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Pickle"
            android:textAppearance="@style/TextAppearance.BharatPickle.TitleLarge"
            android:textColor="@color/black"
            android:drawableEnd="@drawable/ic_baseline_keyboard_arrow_right_24"
            android:padding="10dp"
            android:layout_marginHorizontal="10dp"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/md_theme_light_primary"/>
        <TextView
            android:id="@+id/chutney"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Chutney"
            android:textAppearance="@style/TextAppearance.BharatPickle.TitleLarge"
            android:textColor="@color/black"
            android:drawableEnd="@drawable/ic_baseline_keyboard_arrow_right_24"
            android:padding="10dp"
            android:layout_marginHorizontal="10dp"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/md_theme_light_primary"/>
        <TextView
            android:id="@+id/papd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Papad"
            android:textAppearance="@style/TextAppearance.BharatPickle.TitleLarge"
            android:textColor="@color/black"
            android:drawableEnd="@drawable/ic_baseline_keyboard_arrow_right_24"
            android:padding="10dp"
            android:layout_marginHorizontal="10dp"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/md_theme_light_primary"/>
        <TextView
            android:id="@+id/murbba"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Aawle ka murbba"
            android:textAppearance="@style/TextAppearance.BharatPickle.TitleLarge"
            android:textColor="@color/black"
            android:drawableEnd="@drawable/ic_baseline_keyboard_arrow_right_24"
            android:padding="10dp"
            android:layout_marginHorizontal="10dp"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/md_theme_light_primary"/>
        <TextView
            android:id="@+id/chips"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Chips"
            android:textAppearance="@style/TextAppearance.BharatPickle.TitleLarge"
            android:textColor="@color/black"
            android:drawableEnd="@drawable/ic_baseline_keyboard_arrow_right_24"
            android:padding="10dp"
            android:layout_marginHorizontal="10dp"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/md_theme_light_primary"/>
        <TextView
            android:id="@+id/hlwa"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Aawle Ka Hlwa"
            android:textAppearance="@style/TextAppearance.BharatPickle.TitleLarge"
            android:textColor="@color/black"
            android:drawableEnd="@drawable/ic_baseline_keyboard_arrow_right_24"
            android:padding="10dp"
            android:layout_marginHorizontal="10dp"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/md_theme_light_primary"/>





    </LinearLayout>




</LinearLayout>